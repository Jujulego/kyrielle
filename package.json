{
  "name": "kyrielle",
  "version": "0.3.0",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/kyrielle"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "default": "./dist/events/index.js"
    },
    "./operators": {
      "types": "./dist/operators/index.d.ts",
      "default": "./dist/operators/index.js"
    },
    "./refs": {
      "types": "./dist/refs/index.d.ts",
      "default": "./dist/refs/index.js"
    },
    "./steps": {
      "types": "./dist/steps/index.d.ts",
      "default": "./dist/steps/index.js"
    },
    "./subscriptions": {
      "types": "./dist/subscriptions/index.d.ts",
      "default": "./dist/subscriptions/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "devDependencies": {
    "@jujulego/jill": "2.3.5",
    "@jujulego/vite-plugin-swc": "1.1.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@types/node": "18.18.12",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.54.0",
    "eslint-plugin-vitest": "0.3.10",
    "shx": "0.3.4",
    "typescript": "5.3.2",
    "vite": "5.0.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.2",
  "dependenciesMeta": {
    "vitest@0.34.6": {
      "unplugged": true
    }
  }
}
