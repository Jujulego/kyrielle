{
  "name": "kyrielle",
  "version": "0.10.1",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/kyrielle"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "./dist",
    "./browser.d.ts",
    "./collections.d.ts",
    "./events.d.ts",
    "./operators.d.ts",
    "./pipe.d.ts",
    "./refs.d.ts",
    "./subscriptions.d.ts"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "default": "./dist/browser/index.js"
    },
    "./collections": {
      "types": "./dist/collections/index.d.ts",
      "default": "./dist/collections/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "default": "./dist/events/index.js"
    },
    "./operators": {
      "types": "./dist/operators/index.d.ts",
      "default": "./dist/operators/index.js"
    },
    "./pipe": {
      "types": "./dist/pipe/index.d.ts",
      "default": "./dist/pipe/index.js"
    },
    "./refs": {
      "types": "./dist/refs/index.d.ts",
      "default": "./dist/refs/index.js"
    },
    "./subscriptions": {
      "types": "./dist/subscriptions/index.d.ts",
      "default": "./dist/subscriptions/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill run 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run --typecheck"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.3",
    "immer": "^10.0.3"
  },
  "devDependencies": {
    "@jujulego/jill": "2.5.0",
    "@jujulego/vite-plugin-swc": "1.1.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.3.6",
    "@swc/core": "1.3.107",
    "@types/node": "18.19.14",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vitest/coverage-v8": "1.2.2",
    "eslint": "8.56.0",
    "eslint-plugin-vitest": "0.3.21",
    "happy-dom": "13.3.8",
    "shx": "0.3.4",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.2"
  },
  "packageManager": "yarn@4.1.0",
  "dependenciesMeta": {
    "vitest@1.2.2": {
      "unplugged": true
    }
  }
}
